  {{- if eq . "solarized" }}
set notermguicolors
let g:yadr_disable_solarized_enhancements = 0
let g:airline_theme = 'solarized_flood'
  {{- else }}
set termguicolors
let g:yadr_disable_solarized_enhancements = 1
let g:airline_theme = '{{ . }}'
  {{- end }}
let g:lightline.colorscheme = '{{ . }}'
colorscheme {{ . }}
